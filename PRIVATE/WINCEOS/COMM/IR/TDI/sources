CDEFINES=$(CDEFINES) -DMEMTRACKING
!ENDIF
!ENDIF

!IF ("$(IRDASTK_LOGDEV)" == "1")
CDEFINES = $(CDEFINES) -DLOGDEV
!ENDIF


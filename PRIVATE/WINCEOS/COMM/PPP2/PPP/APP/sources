TARGETNAME=ppp2app
TARGETTYPE=LIBRARY

CDEFINES = $(CDEFINES) -DWINCEAFDMACRO -DWIN32_NO_OLE -DARPSENDAPI
NOMIPS16CODE=1

!IF ("$(MEMTRACKING)" == "1")
CDEFINES = $(CDEFINES) -DMEMTRACKING
!ENDIF


INCLUDES= \
	..\inc; \
	..\..\..\inc; \
	..\..\..\tcpipw\inc; \
	..\..\..\security\inc;

!IFDEF SP_REF_VERBOSE
CDEFINES=$(CDEFINES) -DSP_REF_VERBOSE
!ENDIF

TARGETDEFNAME=ppp
DEFFILE=ppp.def
PREPROCESSDEFFILE=1
COPYRES=1

SOURCES=       \
	dhcp.c		\
	ip_intf.c  	\
	layerfsm.c	\
	option.c	\
	ppp.c      	\
        pppinit.c       \
        ppp2app.rc	\
	util.c     	\

WINCETARGETFILE0=$(_RELEASELIBDIR)\$(TARGETDEFNAME).def

WINCETARGETFILES=$(_RELEASELIBDIR)\$(TARGETNAME).res




TARGETNAME=coremain
TARGETDEFNAME=coredll
DEFFILE=coredll.def
TARGETTYPE=LIBRARY
PREPROCESSDEFFILE=1

CDEFINES=-DWINCEMACRO -DCOREDLL
NOMIPS16CODE=1
INCLUDES=..\inc;..\..\inc;..\..\..\inc;..\..\nk\inc

CONDITIONAL_INCLUDES=shx\shx.h

!IF "$(_TGTCPU)" == "SH4"
ADEFINES=$(ADEFINES) -DSH_CPU=64
!ENDIF
!IF "$(_TGTCPU)" == "SH3"
ADEFINES=$(ADEFINES) -DSH_CPU=48
!ENDIF
!IF "$(_TGTCPU)" == "SH3e"
ADEFINES=$(ADEFINES) -DSH_CPU=62
!ENDIF

CFPUDEFINES=-DMIPS_HAS_FPU

SOURCES= \
    apis.c     \
    showerr.c  \
    coredll.c  \
    time.c     \
    random.c   \
    profiler.c \
    strings.c  \
    strsafew.c \
    username.c \
	CePtrCoredll.cpp

MIPS_SOURCES= \
    mips\intrlock.s \
    mips\chandler.c

SH_SOURCES= \
    sh\intrlock.src \
    sh\chandler.c

PPC_SOURCES= \
    ppc\chandler.c \
    ppc\jmpuwind.s


ARM_SOURCES= \
    arm\intrlock.s \
    arm\chandler.c

x86_SOURCES= \
    x86\exsup.c

WINCETARGETFILE0=$(_COMMONOAKROOT)\lib\$(_CPUINDPATH)\coredll.def


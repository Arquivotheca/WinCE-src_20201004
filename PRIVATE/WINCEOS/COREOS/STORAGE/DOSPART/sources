WINCEOEM=1
IMGNODFLTDDK=1

!IF "$(_TGTOS)"=="CE"
TARGETNAME=mspart
TARGETTYPE=LIBRARY
INCLUDES= ..\inc;

TARGETDEFNAME=mspart
DEFFILE=mspart.def
WINCETARGETFILE0=$(_COMMONOAKROOT)\lib\$(_CPUINDPATH)\mspart.def

!ELSE
TARGETNAME=mspart_nt
TARGETTYPE=DYNLINK
DLLENTRY=DllMain
INCLUDES= ..\nt; ..\inc

TARGETDEFNAME=mspart


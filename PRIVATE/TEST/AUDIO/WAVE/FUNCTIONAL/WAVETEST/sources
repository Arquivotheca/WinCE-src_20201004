WINCEOEM=1

TARGETNAME=wavetest
TARGETTYPE=DYNLINK

SOURCES=\
   tuxmain.cpp \
   errormap.cpp \
   test_wavetest.cpp \
   test_wavein.cpp

!IF "$(_TGTOS)" == "NT" || "$(_TGTOS)" == "NTANSI"

TARGETTHREADMODEL=MT

SDKINC=$(_SDKROOT)\$(_TGTOS)\inc
OAKLIB=$(_PROJECTOAKROOT)\lib\$(_CPUINDPATH)
SDKLIB=$(_SDKROOT)\$(_TGTOS)\lib\x86

INCLUDES= $(SDKINC); \

TARGETLIBS=\
   $(OAKLIB)\kato.lib \
   $(SDKLIB)\winmm.lib \
          
!ELSE

SDKINC=$(_PROJECTSDKROOT)\inc
OAKINC=$(_PROJECTOAKROOT)\inc
SDKLIB=$(_COMMONSDKROOT)\lib\$(_CPUINDPATH)
OAKLIB=$(_PROJECTOAKROOT)\lib\$(_CPUINDPATH)

INCLUDES= $(SDKINC); \
          $(OAKINC); \

DLLENTRY=_DllMainCRTStartup

TARGETLIBS=\
   $(SDKLIB)\coredll.lib \
   $(OAKLIB)\kato.lib \

!ENDIF

#include <windows.h>

//Update the file version number every time a test is  
//changed in a way that will affect performance measurements
#define VERSION_NUM1 	1
#define VERSION_NUM2 	0
#define VERSION_STRING 	"1.0\0"	//this should be consistent with the above numbers

#define FILENAME 	"perf_bluetooth.dll\0"
#define FILEDESCRIPTION "Bluetooth Connectvitiy Setup Teardown Tests\0"
#define COPYRIGHT	"Copyright © 2001\0"


VS_VERSION_INFO VERSIONINFO
 FILEVERSION VERSION_NUM1,VERSION_NUM2	
 FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x40004L
 FILETYPE 0x2L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", "Microsoft\0"
            VALUE "FileDescription", FILEDESCRIPTION 
            VALUE "LegalCopyright", COPYRIGHT	
            VALUE "OriginalFilename", FILENAME 
            VALUE "FileVersion", VERSION_STRING 	
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END



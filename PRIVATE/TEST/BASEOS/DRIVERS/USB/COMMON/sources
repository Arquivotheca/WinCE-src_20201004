	$(_COMMONSDKROOT)\lib\$(_CPUINDPATH)\kernel32.lib \
	$(_COMMONSDKROOT)\lib\$(_CPUINDPATH)\user32.lib \
	$(_PROJECTOAKROOT)\lib\$(_CPUINDPATH)\kato.lib


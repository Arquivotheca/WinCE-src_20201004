[HKEY_LOCAL_MACHINE\Comm\XNIC]
    "DisplayName"="ndis experimental network interface card"
    "Group"="NDIS"
    "ImagePath"="xnic.dll"

[HKEY_LOCAL_MACHINE\Comm\XNIC\Linkage]
    "Route"=multi_sz:"XNIC1"

[HKEY_LOCAL_MACHINE\Comm\XNIC1]
    "DisplayName"="ndis experimental network interface card"
    "Group"="NDIS"
    "ImagePath"="xnic.dll"

[HKEY_LOCAL_MACHINE\Comm\XNIC1\Parms]
    "BusNumber"=dword:0
    "BusType"=dword:0

[HKEY_LOCAL_MACHINE\Comm\XNIC1\Parms\TCPIP]
   "EnableDHCP"=dword:1
   "AutoCfg"=dword:1

<TestSuite
    TestDllName      = "WQMAuthMatrixTest.dll"
    TestFacFuncName  = "CreateAuthMatrixTestSuite"

    PassCriteria = "95"
    FailCriteria = "80"
    TestWeight   = "2.0"

    NumberTestPasses = "1"
    IsBadModes       = "false"
    IsBadKey         = "false"
    IsStressTest     = "false"

    DisableCertEnroll	= "false"
    DisableAuthOpen     = "false"
    DisableAuthShared   = "false"
    DisableAuth802      = "false"
    DisableAuthEAP      = "false"
    DisableAuthPSK      = "false"
    DisableAuthWPA2     = "false"
    DisableCipherClear  = "false"
    DisableCipherWEP    = "false"
    DisableCipherTKIP   = "false"
    DisableCipherAES    = "false"
    DisableEapAuthTLS   = "false"
    DisableEapAuthPEAP  = "false"
    DisableEapAuthMD5   = "false"
    PrefillEapCred      = "false"
>

    <Config>
        <Filter
            inputFile  = "$(ScriptDirName)\APConfig.xml"
            outputFile = "$(TempDirName)\tempConfig.xml"
        />
        <Command>
            WiFiTool -adapter $(AdapterName) -power on -reset
                     -ejectxwifi -disablecustfeedback -disablecertinstall -disableitgproxy -displaytimeout never -IHVExt clear
        </Command>
      <Command>
        WiFiTool -adapter $(AdapterName) -connect -SSID $(FixedAPSSID) -Auth Open -Encr ClearText -tConnTime 80
      </Command>
        <Command>
            APCTool -v
                    -i $(TempDirName)\tempConfig.xml
                    $(LANServerHostArg)
                    $(LANServerPortArg)
                    $(WiFiServerHostArg)
                    $(WiFiServerPortArg)
                    $(WiFiServerSSIDArg)
                    $(WiFiServerAuthArg)
                    $(WiFiServerEncrArg)
                    $(WiFiServerEapAuthArg)
                    $(WiFiServerKeyIndexArg)
                    $(WiFiServerKeyArg)
        </Command>
    </Config>


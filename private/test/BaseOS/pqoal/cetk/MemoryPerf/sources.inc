      /analyze:stacksize16388

!IF "$(ARM_PREFETCH_CACHELINE)"!=""
ARMV5_ASMINCLUDES=$(ARMV5_ASMINCLUDES) -pd "PREFETCH_1CACHELINE SETA $(ARM_PREFETCH_CACHELINE)"
ARMV6_ASMINCLUDES=$(ARMV6_ASMINCLUDES) -pd "PREFETCH_1CACHELINE SETA $(ARM_PREFETCH_CACHELINE)"
ARMV7_ASMINCLUDES=$(ARMV7_ASMINCLUDES) -pd "PREFETCH_1CACHELINE SETA $(ARM_PREFETCH_CACHELINE)"

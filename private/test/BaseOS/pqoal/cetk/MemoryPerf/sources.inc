ARMV7_ASMINCLUDES=$(ARMV7_ASMINCLUDES) -pd "PREFETCH_1CACHELINE SETA 0"
!ENDIF

ARM_BUILD_ASSEMBLY_FILES=1
!IF "$(ARM_BUILD_ASSEMBLY_FILES)"!=""
ARMV5_ASMINCLUDES=$(ARMV5_ASMINCLUDES) -pd "ARM_BUILD_ASSEMBLY_FILES SETA $(ARM_BUILD_ASSEMBLY_FILES)"
ARMV6_ASMINCLUDES=$(ARMV6_ASMINCLUDES) -pd "ARM_BUILD_ASSEMBLY_FILES SETA $(ARM_BUILD_ASSEMBLY_FILES)"
ARMV7_ASMINCLUDES=$(ARMV7_ASMINCLUDES) -pd "ARM_BUILD_ASSEMBLY_FILES SETA $(ARM_BUILD_ASSEMBLY_FILES)"
!ELSE

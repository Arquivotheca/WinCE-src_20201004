TARGETNAME=nkstub
WINCECPU=1
TARGETTYPE=LIBRARY
CDEFINES=$(CDEFINES) -DIN_KERNEL -DWINCEMACRO
NOMIPS16CODE=1

WARNLEVEL=4

# Machine independent defines
CDEFINES=$(CDEFINES) -DDBGSUPPORT
INCLUDES=..\inc;..\..\inc

!IF "$(_TGTCPUFAMILY)"=="MIPS"
WARNISERROR=0
!ENDIF

!IF "$(_TGTCPU)" == "SH4"
ADEFINES=$(ADEFINES) -DSH_CPU=64
!ENDIF

!IF "$(_TGTCPUFAMILY)" == "ARM"
ADEFINES=-pd "_TGTCPU SETS \"$(_TGTCPU)\"" -ignore 241 $(ADEFINES)

!ELSEIF "$(_TGTCPUFAMILY)" == "x86"

CDEFINES=$(CDEFINES) -Gs100000

!ENDIF

SOURCES= \
    nkstub.c \
    kitlstub.c
    
ARM_SOURCES=     \
    arm\armpcb.s \
    arm\dabort.s \
    arm\intrlock.s

SH_SOURCES= \
    sh\intrlock.src

MIPS_SOURCES= \
    mips\intrlock.s


TARGETNAME=nkldr
WINCECPU=1
TARGETTYPE=LIBRARY
CDEFINES=$(CDEFINES) -DIN_KERNEL -DNK_LOADER

WARNLEVEL=4

# Machine independent defines
CDEFINES=$(CDEFINES) -DDBGSUPPORT
INCLUDES=..\inc;..\..\inc

!IF "$(_TGTCPU)" == "SH4"
ADEFINES=$(ADEFINES) -DSH_CPU=64
!ENDIF
!IF "$(_TGTCPU)" == "SH3"
ADEFINES=$(ADEFINES) -DSH_CPU=48
!ENDIF
!IF "$(_TGTCPU)" == "SH3e"
ADEFINES=$(ADEFINES) -DSH_CPU=62
!ENDIF
!IF "$(_TGTCPUFAMILY)" == "MIPS"
ADEFINES=$(ADEFINES) -wd 4710
!ENDIF


ASM_SAFESEH=1

SOURCES= \
    ldrcmn.c \
    strings.c \
    kdinfo.c

x86_SOURCES=     \
    x86\x86init.c    \
    x86\x86Start.asm 

ARM_SOURCES=     \
    arm\arminit.c    \
    arm\armstart.s 

MIPS_SOURCES=   \
    mips\mipstart.s

SH_SOURCES=   \
    sh\shstart.src


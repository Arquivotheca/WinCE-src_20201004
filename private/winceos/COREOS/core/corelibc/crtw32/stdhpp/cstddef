// cstddef standard header
#pragma once
#ifndef _CSTDDEF_
#define _CSTDDEF_
#ifndef RC_INVOKED
#include <yvals.h>

#ifdef _STD_USING
 #undef _STD_USING
  #include <stddef.h>
 #define _STD_USING

#else /* _STD_USING */
 #include <stddef.h>
#endif /* _STD_USING */

 #if _GLOBAL_USING
_STD_BEGIN
using _CSTD ptrdiff_t; using _CSTD size_t;
_STD_END
 #endif /* _GLOBAL_USING */

 #if _HAS_CPP0X
_STD_BEGIN
typedef double max_align_t;	// most aligned type
_STD_END
 #endif /* _HAS_CPP0X */
#endif /* RC_INVOKED */
#endif /* _CSTDDEF_ */

